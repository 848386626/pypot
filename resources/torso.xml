<Robot name="Torsettia">
    <EEPROM>
        <return_delay_time>0</return_delay_time>
        <max_torque>100</max_torque>
        <status_return_level>2</status_return_level>
    </EEPROM>
    
    <DynamixelController type="USB2DXL" port="/dev/ttyUSB0">
        <DynamixelMotor name="left_torso_bend" id="14" type="RX-64" orientation="direct">
            <angle_limits>(-50, 50)</angle_limits>
        </DynamixelMotor>
      
        <DynamixelMotor name="left_shoulder_shrug" id="13" type="RX-64" orientation="indirect">
            <angle_limits>(-30, 130)</angle_limits>
        </DynamixelMotor>
        
        <DynamixelMotor name="left_shoulder_rotate" id="21" type="RX-64" orientation="direct">
            <angle_limits>(-30, 130)</angle_limits>
        </DynamixelMotor>
        
        <DynamixelMotor name="left_shoulder_raise" id="11" type="RX-64" orientation="direct">
            <angle_limits>(-100, 100)</angle_limits>
        </DynamixelMotor>
        
        <DynamixelMotor name="left_elbow_rotate" id="25" type="RX-28" orientation="indirect">
            <angle_limits>(-150, 150)</angle_limits>
        </DynamixelMotor>
        
        <DynamixelMotor name="left_elbow_raise" id="26" type="RX-28" orientation="indirect">
            <angle_limits>(-50, 90)</angle_limits>
        </DynamixelMotor>
        
        <DynamixelMotor name="left_wrist_rotate" id="27" type="RX-28" orientation="indirect">
            <angle_limits>(-150, 30)</angle_limits>
        </DynamixelMotor>
        
        <DynamixelMotor name="left_claw_close" id="28" type="RX-28" orientation="direct">
        </DynamixelMotor>
        
    </DynamixelController>
</Robot>